   34:     # Allow themes to override sidebar views
   35:     view_root = File.expand_path(sidebar.view_root)
   36      rails_root = File.expand_path(::Rails.root.to_s)
   37      if view_root =~ /^#{Regexp.escape(rails_root)}/
