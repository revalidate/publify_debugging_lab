   11:   def render_sidebars(*sidebars)
   12:     rendered_sidebars = (sidebars.blank? ? Sidebar.order(:active_position) : sidebars).map do |sb|
   13:       @sidebar = sb
   14        sb.parse_request(content_array, params)
   15:       render_sidebar(sb)
   16      end
   17:     safe_join rendered_sidebars
   18    rescue => e
   19      logger.error e
   20      logger.error e.backtrace.join("\n")
   21:     I18n.t('errors.render_sidebar')
   22    end
   23  
   24:   def render_sidebar(sidebar)
   25:     if sidebar.view_root
   26:       render_deprecated_sidebar_view_in_view_root sidebar
   27      else
   28:       render_to_string(partial: sidebar.content_partial, locals: sidebar.to_locals_hash, layout: false).html_safe
   29      end
   30    end
   31  
